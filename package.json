{
    "name": "project-root",
    "version": "1.0.0",
    "scripts": {
        "dev:client": "cd client && next dev",
        "build:client": "cd client && next build",
        "start:client": "cd client && next start",
        "dev:backend": "cd backend && next dev",
        "build:backend": "cd backend && next build",
        "start:backend": "cd backend && next start",
        "dev": "concurrently \"npm run dev:client\" \"npm run dev:backend\""
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "dependencies": {
        "@openapitools/openapi-generator-cli": "^2.13.9",
        "swagger-ui-express": "^5.0.1"
    }
}
